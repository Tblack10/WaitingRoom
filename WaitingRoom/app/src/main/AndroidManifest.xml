<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.waitingroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CallerDetailActivity"></activity>
        <activity android:name=".CallerListActivity" />
        <activity android:name=".BusinessesOwnedActivity" />
        <activity android:name=".BusinessLoginActivity">
        </activity>
        <activity android:name=".CallQueueActivity" />
        <activity android:name=".CallRequestActivity" />
        <activity android:name=".BusinessListActivity" />
        <activity android:name=".BusinessSearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
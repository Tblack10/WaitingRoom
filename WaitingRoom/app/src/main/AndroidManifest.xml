<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.waitingroom">
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".business.BusinessCreateUserActivity"></activity>
        <activity android:name=".business.BusinessRegistrationActivity" />
        <activity android:name=".customer.PositionInLIneActivity" />
        <activity android:name=".business.CallerDetailActivity" />
        <activity android:name=".business.CallerListActivity" />
        <activity android:name=".business.BusinessesOwnedActivity" />
        <activity android:name=".business.BusinessLoginActivity" />
        <activity android:name=".business.CallQueueActivity" />
        <activity android:name=".customer.CallRequestActivity" />
        <activity android:name=".business.BusinessListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>